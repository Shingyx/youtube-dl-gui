{
  "defaultSeverity": "error",
  "extends": [
    "tslint:latest",
    "tslint-config-prettier"
  ],
  "jsRules": {},
  "rules": {
    "await-promise": true,
    "no-floating-promises": true,
    "no-implicit-dependencies": [true, "dev"],
    "no-submodule-imports": false,
    "object-literal-sort-keys": false,
    "prefer-const": false,
    "typedef": [
      true,
      "call-signature",
      "member-variable-declaration"
    ]
  },
  "rulesDirectory": []
}
