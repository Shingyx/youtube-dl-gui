{
  "name": "youtube-dl-gui",
  "version": "1.2.0",
  "description": "GUI for youtube-dl",
  "license": "MIT",
  "author": "Su-Shing Chen",
  "scripts": {
    "dist": "electron-webpack && electron-builder",
    "dist:dir": "yarn dist --dir -c.compression=store",
    "format": "yarn tslint --fix && yarn prettier --write",
    "prettier": "prettier --check \"{src,test}/**/*.{ts,tsx,css}\"",
    "start": "electron-webpack dev",
    "test": "jest",
    "posttest": "yarn tslint && yarn prettier",
    "tslint": "tslint --project tsconfig.json"
  },
  "dependencies": {
    "electron-updater": "^4.0.6",
    "electron-window-state": "^5.0.3",
    "node-fetch": "^2.6.7",
    "rc-progress": "^2.3.0",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "react-toastify": "^4.5.2",
    "react-virtualized": "^9.21.0",
    "source-map-support": "^0.5.10",
    "yauzl": "^2.10.0"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "@types/electron-window-state": "^2.0.33",
    "@types/jest": "^24.0.23",
    "@types/node": "^10.12.29",
    "@types/node-fetch": "^2.1.6",
    "@types/react": "^16.8.6",
    "@types/react-dom": "^16.8.2",
    "@types/react-toastify": "^4.0.2",
    "@types/react-virtualized": "^9.18.12",
    "@types/semver": "^5.5.0",
    "@types/yauzl": "^2.9.1",
    "electron": "^3.0.9",
    "electron-builder": "^20.38.5",
    "electron-webpack": "2.6.2",
    "electron-webpack-ts": "^3.1.1",
    "jest": "^24.9.0",
    "prettier": "^1.16.4",
    "rimraf": "^2.6.3",
    "ts-jest": "^24.0.0",
    "tslint": "^5.13.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.3.3333",
    "webpack": "^4.29.6"
  },
  "resolutions": {
    "html-loader": "Shingyx/html-loader#update-deps-with-dist"
  },
  "jest": {
    "preset": "ts-jest"
  },
  "build": {
    "appId": "com.github.shingyx.youtubedlgui",
    "productName": "YouTube DL GUI",
    "nsis": {
      "deleteAppDataOnUninstall": true
    }
  }
}
