{
  "name": "youtube-dl-gui",
  "version": "1.0.0",
  "description": "GUI for youtube-dl",
  "scripts": {
    "start": "electron-webpack dev",
    "dist": "electron-webpack && electron-builder",
    "dist:dir": "npm run dist -- --dir -c.compression=store",
    "format": "npm run prettier && npm run tslint",
    "prettier": "prettier --write \"src/**/*.{ts,tsx,css}\"",
    "tslint": "tslint --fix --project tsconfig.json"
  },
  "author": "Su-Shing Chen",
  "license": "ISC",
  "dependencies": {
    "electron-updater": "^3.2.3",
    "electron-window-state": "^5.0.2",
    "rc-progress": "^2.2.6",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-toastify": "^4.4.3",
    "react-virtualized": "^9.21.0",
    "request": "^2.88.0",
    "source-map-support": "^0.5.9",
    "yauzl": "^2.10.0"
  },
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@babel/preset-react": "^7.0.0",
    "@types/electron-window-state": "^2.0.33",
    "@types/node": "^10.12.7",
    "@types/react": "^16.7.6",
    "@types/react-dom": "^16.0.9",
    "@types/react-toastify": "^4.0.1",
    "@types/react-virtualized": "^9.18.7",
    "@types/request": "^2.48.1",
    "@types/semver": "^5.5.0",
    "@types/yauzl": "^2.9.0",
    "electron": "^3.0.9",
    "electron-builder": "^20.34.0",
    "electron-webpack": "2.3.1",
    "electron-webpack-ts": "^3.0.0",
    "prettier": "^1.15.2",
    "rimraf": "^2.6.2",
    "tslint": "^5.11.0",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^3.1.6",
    "webpack": "^4.25.1"
  },
  "build": {
    "appId": "com.github.shingyx.youtubedlgui",
    "productName": "YouTube DL GUI",
    "nsis": {
      "deleteAppDataOnUninstall": true
    }
  }
}
